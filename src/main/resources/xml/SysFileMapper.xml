<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yww.file.mapper.SysFileMapper">

    <select id="selectByHash" resultType="com.yww.file.entity.SysFile">
        SELECT * FROM sys_file WHERE hash = ${hash} LIMIT 1
    </select>

</mapper>
